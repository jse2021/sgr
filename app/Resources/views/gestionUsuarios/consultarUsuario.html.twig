{% extends 'bases/base.html.twig' %}
{% form_theme form 'form/usuarioForm.html.twig' %}
{% block titulo %}
    Consultar Usuario
{% endblock %}
{% block contenido %}

    <div class="container">
    <h1 class="text-center display-4">
      Consultar Usuario
    </h1>

    <div class="row">

      <div class="col-md-8">
      {% for message in app.flashes('warning')%}
        <div class="alert alert-warning">
            {{message}}
        </div>
        
      {% endfor %}
        {{ form_start(form) }}
        {{ form_widget(form) }}
        {{ form_end(form) }}
      </div>

    </div>
  </div>

{% endblock %}
